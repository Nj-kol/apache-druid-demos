{
   "type":"kafka",
   "spec":{
      "dataSchema":{
         "dataSource":"network_flow",
         "timestampSpec":{
            "format":"iso",
            "column":"ts"
         },
         "dimensionsSpec":{
            "dimensions":[
               {
                  "name":"srcIP",
                  "type":"string"
               },
               {
                  "name":"srcPort",
                  "type":"long"
               },
               {
                  "name":"dstIP",
                  "type":"string"
               },
               {
                  "name":"dstPort",
                  "type":"long"
               },
               {
                  "name":"protocol",
                  "type":"string"
               }
            ]
         },
         "metricsSpec":[
            {
               "type":"count",
               "name":"count"
            },
            {
               "type":"longSum",
               "name":"packets",
               "fieldName":"packets"
            },
            {
               "type":"longSum",
               "name":"bytes",
               "fieldName":"bytes"
            },
            {
               "type":"doubleSum",
               "name":"cost",
               "fieldName":"cost"
            }
         ],
         "granularitySpec":{
            "type":"uniform",
            "segmentGranularity":"HOUR",
            "queryGranularity":"MINUTE",
            "rollup":true
         }
      },
      "ioConfig":{
         "topic":"network-flow",
		 "inputFormat": {
		 	"type": "json"
		 },
         "consumerProperties":{
            "bootstrap.servers":"LM0001680:39092"
         }
      }
   }
}
